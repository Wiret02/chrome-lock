{
    "manifest_version": 3,
    "name": "Chrome Lock Extension",
    "version": "1.0",
    "description": "Lock Chrome with a password.",
    "permissions": ["storage", "activeTab"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    }
  }